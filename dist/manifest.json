{
  "manifest_version": 2,
  "name": "LGTM",
  "description": "Copy animated GIF URL with markdown.",
  "version": "0.0.2",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>"
  ]
}